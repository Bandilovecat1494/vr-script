local RightArm = Instance.new("Part")
RightArm.Size = Vector3.new(1,2,1)
local LeftArm = Instance.new("Part")
LeftArm.Size = Vector3.new(1,2,1)
local Camera = game.Workspace.CurrentCamera
LeftArm.Parent = Camera
RightArm.Parent = Camera
LeftArm.CanCollide = false
RightArm.CanCollide = false
Camera.HeadScale = 3
local HeadPosition = nil
local function Track(Type, Value)
	if Type == Enum.UserCFrame.RightHand then
		RightArm.CFrame = Camera.CoordinateFrame * (CFrame.new(Value.p*(Camera.HeadScale-1))*Value) * CFrame.Angles(90,math.pi,0)
	end
	if Type == Enum.UserCFrame.LeftHand then
		LeftArm.CFrame = Camera.CoordinateFrame * (CFrame.new(Value.p*(Camera.HeadScale-1))*Value) * CFrame.Angles(90,math.pi,0)
	end
	if Type == Enum.UserCFrame.Head then
		HeadPosition = Camera.CoordinateFrame * (CFrame.new(Value.p*(Camera.HeadScale-1))*Value)
		CameraHead.CFrame = HeadPosition
	end
end
Camera.CameraSubject = nil
local InputService = game.UserInputService
InputService.UserCFrameChanged:Connect(Track)
game.Players.LocalPlayer.Character["Left Arm"]:BreakJoints()
game.Players.LocalPlayer.Character.Head:BreakJoints()
game.Players.LocalPlayer.Character["Right Arm"]:BreakJoints()
game["Run Service"].Heartbeat:Connect(function()
	game.Players.LocalPlayer.Character["Right Arm"].CFrame = LeftArm.CFrame
	game.Players.LocalPlayer.Character["Left Arm"].CFrame = RightArm.CFrame
	game.Players.LocalPlayer.Character.Head.CFrame = HeadPosition
	game.Players.LocalPlayer.Character.Torso.Position = Vector3.new(TorsoPos.Position.X , 0 , TorsoPos.Position.Z)
	game.Players.LocalPlayer.Character.Torso.Position.Y = TorsoPos.Position.Y - 3
	game.Players.LocalPlayer.Character.Torso.Velocity = Vector3.new(90,0,8)
	game.Players.LocalPlayer.Character.Head.Velocity = Vector3.new(90,0,8)
	game.Players.LocalPlayer.Character["Left Arm"].Velocity = Vector3.new(90,0,8)
	game.Players.LocalPlayer.Character["Right Arm"].Velocity = Vector3.new(90,0,8)
end)
